<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.bangkit.idku.app">

    <uses-sdk tools:overrideLibrary="com.google.zxing.client.android" />

    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:name=".IndonesiaKu"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.IndonesiaKu">
        <activity
            android:name=".home.profile.ChangePasswordActivity"
            android:label="@string/title_change_password"/>
        <activity
            android:name=".home.profile.EditProfileActivity"
            android:label="@string/title_edit_profile"/>
        <activity android:name=".home.scan_result.ScanResultActivity" />
        <activity android:name=".onboard.ui.pin.newpin.NewPinActivity" />
        <activity android:name=".onboard.ui.pin.entryotp.EntryOtpActivity" />
        <activity android:name=".onboard.ui.pin.forgetpin.ForgetPinActivity" />
        <activity android:name=".onboard.ui.pin.PinActivity" />
        <activity
            android:name=".onboard.ui.OnboardActivity"
            android:label="@string/title_activity_onboarding"
            android:theme="@style/Theme.IndonesiaKu.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".home.HomeActivity"
            android:label="@string/title_activity_home" />
    </application>

</manifest>